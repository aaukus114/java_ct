
public class Monster extends Character{
	int acc;
	static int hp,i;
	
	public Monster(int hp,int acc) {
		this.hp = hp;
		this.acc = acc;
	}
	

	@Override
	public int Acctack(int acc) {
		// TODO Auto-generated method stub

		if(this.i >=1)
		{
			this.hp -= acc;
		}
		else
		{
			this.hp =hp;
			this.hp -= acc;
			this.i++;
		}
		return hp;
	}
}
