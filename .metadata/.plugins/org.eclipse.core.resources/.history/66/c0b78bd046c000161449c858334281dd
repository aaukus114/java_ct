import java.util.Scanner;


public class EX01 {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int year = sc.nextInt();


		if(year %4 == 0)
		{
			if(year %100 == 0)
				System.out.println("Æò³â");
			else
				System.out.println("À±³â");
		}
		else
		{
			if(year %400 == 0)
				System.out.println("À±³â");
		}
	
	}
}
