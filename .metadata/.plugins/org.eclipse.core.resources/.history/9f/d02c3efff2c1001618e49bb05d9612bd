
public class Player extends Character{
	static int hp,i=0;
	int  acc;
	
	public Player(int hp,int acc) {
		this.hp =hp;
		this.acc = acc;
	}

	@Override
	public int Acctack(int acc) {
		// TODO Auto-generated method stub

		if(this.i >=1)
		{
			this.hp -= acc;
		}
		else
		{
			this.hp =hp;
			this.hp -= acc;
			this.i++;
		}
		return hp;
	}

}
